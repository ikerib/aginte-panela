{% extends 'demo.html.twig' %}

{% block body %}
    <h1>Ekintzamotum</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ekintzaMotum.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ekintzaMotum.name }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if ekintzaMotum.created %}{{ ekintzaMotum.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if ekintzaMotum.updated %}{{ ekintzaMotum.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ ekintzaMotum.orden }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_ekintzamota_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_ekintzamota_edit', { 'id': ekintzaMotum.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
