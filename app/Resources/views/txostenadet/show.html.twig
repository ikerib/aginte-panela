{% extends 'front.html.twig' %}

{% block body %}
    <h1>Txostenadet</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ txostenadet.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ txostenadet.name }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if txostenadet.created %}{{ txostenadet.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if txostenadet.updated %}{{ txostenadet.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ txostenadet.orden }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('txostenadet_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('txostenadet_edit', { 'id': txostenadet.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
